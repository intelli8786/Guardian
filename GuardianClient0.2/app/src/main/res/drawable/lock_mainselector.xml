<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true"
        android:state_selected="true"
        android:drawable="@drawable/lock_pressedunlocked"/>
    <item
        android:state_pressed="true"
        android:state_selected="false"
        android:drawable="@drawable/lock_pressedlocked"/>
    <item
        android:state_selected="false"
        android:drawable="@drawable/lock_unpressedlocked"/>
    <item
        android:state_selected="true"
        android:drawable="@drawable/lock_unpressedunlocked"/>
</selector>